// Import all partials
@require './_*.styl'

html, body, #root, .app
	height 100%
	width 100%
	

.padder
	padding 10%
	
//Navbar Div
.navbar
	margin-bottom 0px !important
.brand-logo
	max-width 100px !important
	padding 15px 15px
	float left
	
//Landing Div
.landing-div
	background-image url('images/img_2_gray_blur_gradient.jpg')	
	background-repeat no-repeat
	background-size 100% auto
	height 100vh
	padding-bottom 10px
	
	@media only screen and (max-width : 480px)
		background-color gray
		background-size auto 100%
	
	@media only screen and (max-width : 700px)
		background-color gray
		background-size auto 100%

.navbar-custom 
	background-color none
	border-color none
	background-image none
	background-repeat no-repeat
	
.femhack-header-img
	display block
	margin 0 auto
	height 40%
	width 60%
	
.challenge-header-img
       display: block
    margin-left: auto
    margin-right: auto 

.header-registration
	text-align:center

//Description Div
.description-div
	background-color bluegrass
	

//About Div
.about-div
	// background-image url('images/img_3_gray_blur_gradient.jpg')	
	background-image url('images/img_5.jpg')	
	background-repeat no-repeat
	// background-size 100% auto
	background-size auto 100%
	text-align center
	
// Challenge Div
.challenge-div
background-color #F8A180

//Contact Div
.contact-div
	// background-color bluegrass
	background-color seagreen
	color white

.contact-info
	padding-top 5%
	
//Footer Div
.footer-div
	background-color black
	padding-top 10px
	
.text-muted
	padding-top 8px
	
.btn-warning
	background-color #FBCD73 !important
	border-color #FBCD73 !important

// Schedule Div
.schedule-div
        background-color #F8A180
.schedule-content
	padding-left 10%
